<Config>
  <LogLevel>${RADARR__LOG_LEVEL:-info}</LogLevel>
  <UrlBase>${RADARR__URL_BASE}</UrlBase>
  <Branch>${RADARR__BRANCH}</Branch>
  <ApiKey>${RADARR__API_KEY}</ApiKey>
  <AuthenticationMethod>${RADARR__AUTHENTICATION_METHOD:-External}</AuthenticationMethod>
  <AuthenticationRequired>${RADARR__AUTHENTICATION_REQUIRED:-DisabledForLocalAddresses}</AuthenticationRequired>
  <InstanceName>${RADARR__INSTANCE_NAME}</InstanceName>
  <PostgresUser>${RADARR__POSTGRES_USER}</PostgresUser>
  <PostgresPassword>${RADARR__POSTGRES_PASSWORD}</PostgresPassword>
  <PostgresPort>${RADARR__POSTGRES_PORT:-5432}</PostgresPort>
  <PostgresHost>${RADARR__POSTGRES_HOST}</PostgresHost>
  <PostgresMainDb>${RADARR__POSTGRES_MAIN_DB}</PostgresMainDb>
  <PostgresLogDb>${RADARR__POSTGRES_LOG_DB}</PostgresLogDb>
  <Port>${RADARR__PORT}</Port>
  <BindAddress>*</BindAddress>
  <LaunchBrowser>False</LaunchBrowser>
  <EnableSsl>False</EnableSsl>
  <SslPort>6969</SslPort>
  <SslCertPath></SslCertPath>
  <SslCertPassword></SslCertPassword>
  <UpdateMechanism>BuiltIn</UpdateMechanism>
  <AnalyticsEnabled>${RADARR__ANALYTICS_ENABLED}</AnalyticsEnabled>
  <Theme>${RADARR__THEME:-auto}</Theme>
</Config>
